{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#141415",
    "bgPanel": "#1c1c24",
    "bgElement": "#333738",
    "fg": "#cdcdcd",
    "fgMuted": "#606079",
    "fgAlt": "#c3c3d5",
    "red": "#d8647e",
    "green": "#7fa563",
    "yellow": "#f3be7c",
    "orange": "#e8b589",
    "blue": "#7e98e8",
    "magenta": "#bb9dbd",
    "purple": "#aeaed1",
    "cyan": "#b4d4cf"
  },
  "theme": {
    "primary": {
      "dark": "blue",
      "light": "blue"
    },
    "secondary": {
      "dark": "magenta",
      "light": "magenta"
    },
    "accent": {
      "dark": "cyan",
      "light": "cyan"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "yellow",
      "light": "yellow"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "blue",
      "light": "blue"
    },
    "text": {
      "dark": "fg",
      "light": "fg"
    },
    "textMuted": {
      "dark": "fgMuted",
      "light": "fgMuted"
    },
    "background": {
      "dark": "bg",
      "light": "bg"
    },
    "backgroundPanel": {
      "dark": "bgPanel",
      "light": "bgPanel"
    },
    "backgroundElement": {
      "dark": "bgElement",
      "light": "bgElement"
    },
    "border": {
      "dark": "fgMuted",
      "light": "fgMuted"
    },
    "borderActive": {
      "dark": "fg",
      "light": "fg"
    },
    "borderSubtle": {
      "dark": "bgElement",
      "light": "bgElement"
    },
    "diffAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffContext": {
      "dark": "fgMuted",
      "light": "fgMuted"
    },
    "diffHunkHeader": {
      "dark": "purple",
      "light": "purple"
    },
    "diffHighlightAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffHighlightRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffAddedBg": {
      "dark": "bgPanel",
      "light": "bgPanel"
    },
    "diffRemovedBg": {
      "dark": "bgPanel",
      "light": "bgPanel"
    },
    "diffContextBg": {
      "dark": "bgPanel",
      "light": "bgPanel"
