{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA",
    "green": "#A6E3A1",
    "yellow": "#F9E2AF",
    "red": "#f38ba8"
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:lavender",
    "template": "\uf105 "
  },
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "foreground": "p:lavender",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "p:lavender",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "display_status": true,
            "display_stash_count": true,
            "fetch_status": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "<p:green>{{ .HEAD }}</> <p:blue>{{ .BranchStatus}}</> {{ if .Working.Changed }}<p:yellow>!{{ add .Working.Unmerged .Working.Deleted .Working.Added .Working.Modified .Working.Untracked }}</>{{ end }}{{if gt .Working.Untracked 0}} <p:blue>?{{ .Working.Untracked}}</>{{end}}{{ if and (.Working.Changed) (.Staging.Changed) }}|{{ end }}{{ if .Staging.Changed }}<p:yellow>+{{ add .Staging.Added .Staging.Deleted .Staging.Modified}}</> {{ end }}{{ if gt .StashCount 0 }} <p:green>*{{ .StashCount }}</>{{ end }}",
          "source": "cli",
          "style": "plain",
          "type": "git"
        },
        {
          "style": "plain",
          "foreground": "p:closer",
          "template": "\n\uf105",
          "type": "text"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 3
}
